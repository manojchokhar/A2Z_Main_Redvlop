<%- include('navbar.ejs') %>

<section>
    <div class="container  pb-5 pt-5 " style="width: 90%;">
        <form action="/admin/update_Our_Vision_section" method="post" enctype="multipart/form-data">
            <div class="container  p-5 bg-secondary navbar-dark" style="box-shadow:0px 5px 10px red;" >
                <div class="row">
                    <div class="pt-3  col-md-12 mt-2 mb-5">
                        <h1 class="text-primary  text-center">Edit Our Vision Section</h1>
                    </div>
                    <div class="pt-3  col-md-6 mt-2">
                        Our Vision Title
                        <input type="text" required name="Edit_Vision_title" class="form-control" value="<%=vision_title%>">
                    </div>
                    <div class="pt-3  col-md-6 mt-2">
                        Our Vision Description
                        <input type="text" name="Edit_Vision_desc" class="form-control" value="<%=vision_desc%>">
                    </div>
                    <div class="col-md-12 mt-5 text-center">
                        <button class="btn btn-md text-white " style="background-color:red ";>Save Our  Vision Infromation</button>
                    </div>
                    <div class="pt-3  col-md-6 mt-2">
                        <input type="hidden"  name="vision_id" class="form-control" value="<%=vision_id%>">
                    </div>
                </div>
        
            </div>
        </form>
    </div>
</section>    
<%- include('footer.ejs') %>
